{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["ES2021"],
    "module": "ES2020",
    "moduleResolution": "node",
    "outDir": "build",
    "rootDir": "src",
    "skipLibCheck": true,
    "target": "ES2021"
  },
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
